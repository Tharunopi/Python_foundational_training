   "id": "cb07e0dd-3d00-46ae-adc5-db20191d90d1",
   "metadata": {},
   "outputs": [],
   "source": [
    "def bubble_sort(x):\n",
    "    len_x = len(x)\n",
    "\n",
    "    for i in range(len_x):\n",
    "        if i+1 < len_x and x[i] > x[i+1]:\n",
    "            x[i], x[i+1] = x[i+1], x[i]\n",
    "\n",
    "    print(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "id": "0b853001-6c69-4567-98cc-70c76776502d",
   "metadata": {},
   "outputs": [
